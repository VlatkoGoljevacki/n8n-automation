    {
      "parameters": {
        "mode": "runOnceForAllItems",
        "jsCode": "// Prepare input for article validation: pass orderLines + context\nconst parseResult = $input.first().json;\nconst emailMeta = $('Extract Email Metadata').first().json;\n\nreturn [{\n  json: {\n    orderLines: parseResult.orderLines || [],\n    mapping: parseResult.mapping || {},\n    lineCount: parseResult.lineCount || 0,\n    senderEmail: emailMeta.senderEmail,\n    emailSubject: emailMeta.emailSubject\n  }\n}];\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1920, -300],
      "id": "b1000000-0000-4000-8000-000000000023",
